{
  "name": "etherspot-lite",
  "version": "0.0.1",
  "description": "Etherspot-Lite SDK",
  "keywords": [
    "ether",
    "ethereum",
    "sdk",
    "layer2",
    "blockchain",
    "account abstraction",
    "4337",
    "etherspot"
  ],
  "license": "MIT",
  "homepage": "https://etherspot.dev",
  "main": "./dist/node.js",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "exit 1",
    "format": "prettier --write \"{src,test,examples}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "prebuild": "rimraf ./dist",
    "build": "npm run compile",
    "compile": "tsc -p ./tsconfig.build.json",
    "compile-watch": "npm run compile -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etherspot/etherspot-lite-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/etherspot/etherspot-lite-sdk/issues"
  },
  "dependencies": {
    "ethers": "^5.7.0",
    "@thehubbleproject/bls": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "13.13.9",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "typescript": "4.5.2",
    "ts-node": "8.10.2"
  }
}
